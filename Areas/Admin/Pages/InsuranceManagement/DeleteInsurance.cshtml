@page "{id:int?}"
@model AssetProject.Areas.Admin.Pages.InsuranceManagement.DeleteInsuranceModel
@{
}
<div class="slim-mainpanel">
      <div class="container">
        <div class="slim-pageheader">
          <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="#">Home</a></li>
            <li class="breadcrumb-item"><a href="#">Set Up</a></li>
            <li class="breadcrumb-item active" aria-current="page">Delete Insurance</li>
          </ol>
          <h6 class="slim-pagetitle">Insurance Policy Information</h6>
        </div><!-- slim-pageheader -->

        <div class="section-wrapper">
          <div class="form-layout">
                
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="row mg-b-20">
                      <div class="col-lg-4">
                        <div class="form-group">
                          <label class="form-control-label">Insurance Title : <span class="tx-danger">*</span></label>
                          <div>
                               <label ><h4>@Model.insurance.Title</h4></label>
                          </div>
                         
                        </div>
                      </div><!-- col-4 -->
              
                      <div class="col-lg-4">
                        <div class="form-group">
                          <label class="form-control-label"> Description : <span class="tx-danger">*</span></label>
                           <div>
                               <label ><h4>@Model.insurance.Description</h4></label>
                          </div>
                        </div>
                      </div>
                      <div class="col-lg-4">
                        <div class="form-group">
                          <label class="form-control-label"> Insurance Company : <span class="tx-danger">*</span></label>
                           <div>
                               <label ><h4>@Model.insurance.InsuranceCompany</h4></label>
                          </div>
                        </div>
                      </div><!-- col-4 -->
                    </div><!--End Row-->
                    <div class="row mg-b-20">
                      <div class="col-lg-4">
                        <div class="form-group mg-b-10-force">
                          <label class="form-control-label">Contact Person : <span class="tx-danger">*</span></label>
                           <div  >
                               <label ><h4>@Model.insurance.ContactPerson</h4></label>
                          </div>
                        </div>
                      </div><!-- col-4 -->
                      <div class="col-lg-4">
                        <div class="form-group">
                          <label class="form-control-label">Phone : <span class="tx-danger">*</span></label>
                           <div >
                               <label><h4 >@Model.insurance.Phone</h4></label>
                          </div>
                        </div>
                      </div><!-- col-4 -->
              
                      <div class="col-lg-4">
                        <div class="form-group">
                          <label class="form-control-label">Policy Number: <span class="tx-danger">*</span></label>
                           <div >
                               <label><h4>@Model.insurance.PolicyNo</h4></label>
                          </div>
                        </div>
                      </div><!-- col-4 -->
                    </div><!--End Row-->
                    <div class="row mg-b-20">
                      <div class="col-lg-6">
                        <div class="form-group">
                          <label class="form-control-label">Start Date : <span class="tx-danger">*</span></label>
                           <div>
                               <label ><h3 >@Model.StartDate</h3></label>
                          </div>
                        </div>
                      </div><!-- col-6 -->
                      <div class="col-lg-6">
                        <div class="form-group">
                          <label class="form-control-label">End Date : <span class="tx-danger">*</span></label>
                          <div>
                               <label ><h4>@Model.EndDate</h4></label>
                          </div>
                        </div>
                      </div><!-- col-6 -->
                    </div><!--End Row-->
                    <div class="row mg-b-20">
                      <div class="col-lg-6">
                        <div class="form-group">
                          <label class="form-control-label">Deductible : <span class="tx-danger">*</span></label>
                           <div>
                               <label ><h4>@Model.insurance.Deductible</h4></label>
                          </div>
                        </div>
                      </div><!-- col-6 -->
                      <div class="col-lg-6">
                         <div class="form-group">
                          <label class="form-control-label">Permium : <span class="tx-danger">*</span></label>
                           <div>
                               <label><h4>@Model.insurance.Permium</h4></label>
                          </div>
                         </div>
                      </div><!-- col-6 -->   
                    </div ><!-- End row -->
                    <div class="form-check">
                            <input  asp-for="@Model.insurance.IsActive" class="form-check-input" disabled>
                            <label class="form-check-label" asp-for="@Model.insurance.IsActive" >  Active  </label>                 
                    </div>
                       
                    <div class="form-layout-footer mt-4 ">
                           <form  method="post" enctype="multipart/form-data" >
                               <input type="text" asp-for="@Model.insurance.InsuranceId" hidden>
                                <input type="submit" value="Delete" class="btn btn-danger" >
                                 <a asp-area="admin" class="btn btn-secondary " asp-page="InsuranceList">Back</a>
                            </form>
                     
                     </div><!-- form-layout-footer -->
             
          </div><!-- form-layout -->
        </div><!-- section-wrapper --> 
  </div><!-- container -->
</div>


