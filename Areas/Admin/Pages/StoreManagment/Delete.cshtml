@page
@model AssetProject.Areas.Admin.Pages.StoreManagment.DeleteModel
@{
}


<div class="slim-mainpanel">
    <div class="container">
        <div class="slim-pageheader">
            <ol class="breadcrumb slim-breadcrumb">
                <li class="breadcrumb-item"><a href="#">Home</a></li>
                <li class="breadcrumb-item"><a href="#">Inventory</a></li>
                <li class="breadcrumb-item active" aria-current="page">Delete Store</li>
            </ol>
            <h6 class="slim-pagetitle">Store Information</h6>
        </div><!-- slim-pageheader -->

        <div class="section-wrapper">
            @* <label class="section-title">Please fill your company information</label>*@
            @*<p class="mg-b-20 mg-sm-b-40">Please fill your company information</p>*@

            <div class="form-layout">
                <div class="row mg-b-25">
                    <div class="col-lg-4">
                        <label class="form-control-label">Store Title: <span class="tx-danger">*</span></label>
                        <div style="font-size:xx-large">@Model.Store.StoreTitle</div>
                    </div><!-- col-4 -->

                    <div class="col-lg-4">
                        <label class="form-control-label">Address: <span class="tx-danger">*</span></label>
                        <div style="font-size:xx-large">@Model.Store.Address</div>
                    </div><!-- col-4 -->


                    <div class="col-lg-4">
                        <label class="form-control-label">Mobile No: <span class="tx-danger">*</span></label>
                        <div style="font-size:xx-large">@Model.Store.Mobile</div>
                    </div><!-- col-4 -->


                    <div class="col-lg-4">
                        <label class="form-control-label">Telephone: <span class="tx-danger">*</span></label>
                        <div style="font-size:xx-large">@Model.Store.Tele</div>
                    </div><!-- col-4 -->

                    <div class="col-lg-4">
                        <label class="form-control-label">Fax: <span class="tx-danger">*</span></label>
                        <div style="font-size:xx-large">@Model.Store.Fax</div>
                    </div><!-- col-4 -->


                    
                </div><!-- row -->

                <div class="form-layout-footer">
                    <form method="post" asp-route-id="@Model.Store.StoreId">
                        <input type="submit" value='Delete' class="btn btn-primary bd-0">

                        <a asp-area="Admin" asp-page="/StoreManagment/List">
                            <input type='button' class="btn btn-secondary bd-0" value='Cancel' />
                        </a>
                    </form>

                </div><!-- form-layout-footer -->

            </div><!-- form-layout -->
        </div><!-- section-wrapper -->

    </div><!-- container -->
</div>


@section Scripts {
    <script>


    </script>
}