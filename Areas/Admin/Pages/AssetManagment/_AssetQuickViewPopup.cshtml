@{
}

<div id="AssetQuickViewPoup" class="modal fade effect-super-scaled" style="display: none;" aria-hidden="true">
    <div class="modal-dialog modal-dialog-vertical-center modal-lg" role="document">
        <div class="modal-content bd-0 tx-14">
            <div class="modal-header pd-y-20 pd-x-25">
                <h6 class="tx-14 mg-b-0 tx-uppercase tx-inverse tx-bold">Message Preview</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body pd-25">
                <div class="row row-sm">
                <div class="col-lg-12">
                    <div class="card card-profile">
                        <div class="card-body">
                            <div class="media">
                                <img src="" id="img" alt="" class="img-fluid" >
                                <div class="media-body">
                                    <h3 class="card-profile-name">Asset Description</h3>
                                    <p class="card-profile-position" >  Asset Tag Id <a href="" id="AssetTagId"></a></p>
                                    <p >Serial <a href="" id="AssetSerial"></a></p>
                                    <div class="row mg-t-20">
                                        <div class="col-md">
                                            <label class="section-label-sm tx-gray-500">General Information</label>
                                            <p class="invoice-info-row">
                                                <span>Description</span>
                                                <span id="AssetDescription"></span>
                                            </p>
                                            <p class="invoice-info-row">
                                                <span>Asset Cost</span>
                                                <span id="AssetCost"></span>
                                            </p>
                                            <p class="invoice-info-row">
                                                <span>Asset PurchaseDate:</span>
                                                <span id="AssetPurchaseDate"></span>
                                            </p>
                                            <p class="invoice-info-row">
                                                <span>Item:</span>
                                                <span id="AssetItem"></span>
                                            </p>
                                            
                                        </div><!-- col -->
                                        <div class="col-md">
                                            <label class="section-label-sm tx-gray-500">Depreciation Information</label>
                                            <p class="invoice-info-row">
                                                <span>Depreciable Cost</span>
                                                <span id="DepreciableCost"></span>
                                            </p>
                                            <p class="invoice-info-row">
                                                <span>Salvage Value</span>
                                                <span id="SalvageValue"></span>
                                            </p>
                                            <p class="invoice-info-row">
                                                <span>Asset Life:</span>
                                                <span id="AssetLife"></span>
                                            </p>
                                            <p class="invoice-info-row">
                                                <span>Date Acquired:</span>
                                                <span id="DateAcquired"></span>
                                            </p>
                                            <p class="invoice-info-row">
                                                <span>Depreciation Method:</span>
                                                <span id="DepreciationMethod"></span>
                                            </p>
                                        </div><!-- col -->
                                    </div>
                                </div><!-- media-body -->
                            </div><!-- media -->
                        </div><!-- card-body -->
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div><!-- modal-dialog -->
</div>
</div>
</div>
</div>

